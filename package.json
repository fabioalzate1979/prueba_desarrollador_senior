{
    "name": "pruebadesarrolladorsenior-template-backend-ms",
    "version": "1.0.0",
    "main": "dist/index.js",
    "description": "Template para microservicios de backend",
    "license": "UNLICENSED",
    "private": true,
    "author": {
        "email": "it@coordinadora.com",
        "name": "Coordinadora Mercantil S.A"
    },
    "scripts": {
        "build": "rimraf dist && tsc -p tsconfig.build.json",
        "lint": "eslint . --ext .ts --fix",
        "format": "prettier --write \"./src/**/*.ts\"",
        "format-check": "prettier --list-different \"./src/**/*.ts\"",
        "dev": "tsc-watch -p tsconfig.build.json --onSuccess \"yarn start-lint-dev\"",
        "start": "node dist/index.js",
        "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/index.js\"",
        "test": "jest --color --verbose --no-cache",
        "coverage": "jest --coverage",
        "release": "standard-version",
        "husky:install": "husky install",
          "deploy": "gcloud run deploy pruebadesarrollosenior --image=gcr.io/pruebadesarrollosenior/app --platform=managed --region=us-central1 --allow-unauthenticated",
        "start-lint-dev": "yarn lint && node dist/index.js",
        "test:mutation": "npx stryker run"
    },
    "dependencies": {
        "@fastify/cors": "8.0.0",
        "@fastify/rate-limit": "10.0.0",
        "@fastify/swagger": "^8.15.0",
        "@fastify/swagger-ui": "^4.1.1",
        "@google-cloud/firestore": "7.10.0",
        "@google-cloud/pubsub": "^4.7.2",
        "@types/named-routes": "^2.0.5",
        "axios": "^1.7.7",
        "body-parser": "^1.20.3",
        "cm-idempotencia": "^1.4.6",
        "dotenv": "^16.4.5",
        "fastify": "^4.28.1",
        "fastify-cors": "^6.1.0",
        "fastify-formbody": "^5.3.0",
        "fastify-helmet": "^7.1.0",
        "fastify-request-logs": "1.3.0",
        "graphql": "^16.9.0",
        "inversify": "6.0.2",
        "jest-junit": "^16.0.0",
        "joi": "^17.13.3",
        "module-alias": "2.2.3",
        "moment-timezone": "^0.5.45",
        "named-routes": "^2.0.7",
        "pg-mem": "^3.0.2",
        "pg-promise": "^11.9.1",
        "redis": "^4.7.0",
        "reflect-metadata": "^0.2.2",
        "winston": "^3.14.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.5.0",
        "@commitlint/config-conventional": "19.5.0",
        "@types/fastify-rate-limit": "^2.1.0",
        "@types/jest": "29.5.13",
        "@types/moment-timezone": "^0.5.30",
        "@types/node": "^22.5.5",
        "@types/redis": "^4.0.11",
        "@types/reflect-metadata": "^0.1.0",
        "@types/supertest": "6.0.2",
        "@typescript-eslint/eslint-plugin": "5.36.1",
        "@typescript-eslint/parser": "5.36.1",
        "eslint": "8.57.1",
        "eslint-config-airbnb-typescript": "^18.0.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.30.0",
        "eslint-plugin-n": "^17.10.3",
        "eslint-plugin-prettier": "5.2.1",
        "eslint-plugin-promise": "^7.1.0",
        "husky": "9.1.6",
        "jest": "29.7.0",
        "lint-staged": "^15.2.10",
        "mock-cloud-firestore": "^0.12.0",
        "openapi-types": "^12.1.3",
        "prettier": "3.3.3",
        "rimraf": "6.0.1",
        "standard-version": "9.5.0",
        "supertest": "^7.0.0",
        "ts-jest": "29.2.5",
        "ts-node": "10.9.2",
        "tsc-watch": "6.2.0",
        "typescript": "5.6.2"
    },
    "_moduleAliases": {
        "@common": "dist/common",
        "@modules": "dist/modules",
        "@application": "dist/application",
        "@domain": "dist/domain",
        "@infrastructure": "dist/infrastructure",
        "@configuration": "dist/configuration",
        "@util": "dist/util",
        "@app": "dist/app"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn format && yarn lint",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "yarn format",
            "yarn lint --fix"
        ]
    }
}
